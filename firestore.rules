rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
    	allow create: if
      	request.auth != null && 
        request.auth.uid == request.resource.data.owner;    
      allow read, write, delete: if 
        request.auth != null && 
        request.auth.uid == resource.data.owner;    
    }
  }
}